<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="32dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true">

        <CheckBox
            android:id="@+id/check_liste_location_liste"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true" />

        <TextView
            android:id="@+id/numero_liste_location_liste_txt"
            android:layout_width="150dp"
            android:layout_height="32dp"
            android:layout_alignParentTop="true"
            android:text="N°Bail" />

        <TextView
            android:id="@+id/commande_liste_location_liste_txt"
            android:layout_width="150dp"
            android:layout_height="32dp"
            android:layout_alignParentTop="true"
            android:text="Commande" />

        <TextView
            android:id="@+id/contrat_liste_location_liste_txt"
            android:layout_width="150dp"
            android:layout_height="32dp"
            android:layout_alignParentTop="true"
            android:text="Contrat" />

        <TextView
            android:id="@+id/client_liste_location_liste_txt"
            android:layout_width="150dp"
            android:layout_height="32dp"
            android:layout_alignParentTop="true"
            android:text="Client" />

        <TextView
            android:id="@+id/date_debut_loc_liste_location_liste_txt"
            android:layout_width="150dp"
            android:layout_height="32dp"
            android:layout_alignParentTop="true"
            android:text="Date Debut Loc" />

        <TextView
            android:id="@+id/date_fin_loc_liste_location_liste_txt"
            android:layout_width="150dp"
            android:layout_height="32dp"
            android:layout_alignParentTop="true"
            android:text="Date fin Loc" />
        <TextView
            android:id="@+id/commercial_liste_location_liste_txt"
            android:layout_width="150dp"
            android:layout_height="32dp"
            android:layout_alignParentTop="true"
            android:text="Commercial" />
        <TextView
            android:id="@+id/unite_liste_location_liste_txt"
            android:layout_width="150dp"
            android:layout_height="32dp"
            android:layout_alignParentTop="true"
            android:text="Unité" />

    </LinearLayout>
</android.support.constraint.ConstraintLayout>